{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/dashboard", "destination": "/dashboard.html" },
    { "source": "/login", "destination": "/loginForPro.html" },
    { "source": "/manufacturer", "destination": "/manufacturer-dashboard.html" },
    { "source": "/manufacturer/login", "destination": "/ureticiLogin.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "no-store" }
      ]
    }
  ]
}
