{
  "version": 2,
  "routes": [
    {
      "src": "/sw.js",
      "headers": {
        "cache-control": "public, max-age=0, must-revalidate",
        "Service-Worker-Allowed": "/"
      },
      "dest": "/sw.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dashboard.html"
    }
  ],
  "env": {
    "API_URL": "https://eticaret-glif.onrender.com/api"
  }
}
